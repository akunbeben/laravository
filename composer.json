{
    "name": "akunbeben/laravository",
    "description": "Simplify Repository Pattern for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Benny Rahmat",
            "email": "akunbeben@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Akunbeben\\Laravository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Akunbeben\\Laravository\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.17"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akunbeben\\Laravository\\LaravositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
